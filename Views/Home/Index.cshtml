    <link rel="stylesheet" href="/Content/Custom_Styles/Home/style.css">
    <link rel="stylesheet" href="/Content/Custom_Styles/Home/responsive.css">

    <script>
        $(document).ready(function () {
            $('#lnkLogin').on('click', function () {
                window.location.href = '/Login/LoginPage';
            });

            $('#lnkHome').on('click', function () {
                window.location.href = '/Home/Index';
            });

            $('#lnkTracking').on('click', function () {
                window.location.href = '/ShipmentHistory/ViewShipmentTracking';
            });

            $('#lnkAbout').on('click', function () {
                window.location.href = '/Home/About';
            });
        });
    </script>

